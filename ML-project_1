        "def add_labels(x, y):\n",
        "    for i in range(len(x)):                            #Функция для отображения числовых значений в центре диаграмм\n",
        "        plt.text(i, y[i] // 2, y[i], ha='center')\n",
        "\n",
        "x = ['positive','negative']\n",
        "y = df.sentiment.value_counts()\n",
        "plt.figure(figsize=(6, 7))\n",
        "plt.bar(x,y, color=['skyblue', 'tomato'])\n",
        "plt.ylabel(\"Количество отзывов\")\n",
        "plt.title(\"Отзывы\")\n",
        "add_labels(x, y)\n",
        "plt.show()"
