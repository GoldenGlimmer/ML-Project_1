        "import re\n",
        "import string\n",
        "import nltk\n",
        "from nltk.corpus import stopwords\n",
        "from nltk.stem import WordNetLemmatizer\n",
        "\n",
        "\n",
        "nltk.download('stopwords')\n",
        "nltk.download('wordnet')\n",
        "\n",
        "stop_words = set(stopwords.words('english'))\n",
        "lemmatizer = WordNetLemmatizer()\n",
        "\n",
        "def preprocess_text(text):\n",
        "    text = text.lower()  # Приводим буквы к нижнему регистру\n",
        "    text = re.sub(r'<.*?>', '', text)  # Удаляем HTML тэги\n",
        "    text = re.sub(r'\\d+', '', text)  # Удаляем числа\n",
        "    text = text.translate(str.maketrans('', '', string.punctuation))  # Удаляем знаки пунктуации\n",
        "    tokens = text.split()\n",
        "    tokens = [lemmatizer.lemmatize(word) for word in tokens if word not in stop_words]  # проводим лемматизацию и удаляем стоп-слова\n",
        "    return ' '.join(tokens)\n",
        "df['review'] = df['review'].apply(preprocess_text)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "ftxcRajsuJW1",
        "outputId": "205fde0c-4cf3-4470-b803-0cd7f1f814f6"
      },
      "execution_count": 13,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stderr",
